        super(name, platformType);
    }

    protected TemporalType(String name, Class platformType, boolean nullable) {
        super(name, platformType, nullable);
    }

    protected TemporalType(String name, Class platformType, int scale, int precision, boolean nullable) {
        super(name, platformType, scale, precision, nullable);
    }
}
