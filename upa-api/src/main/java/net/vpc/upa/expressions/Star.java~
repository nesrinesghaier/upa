package net.vpc.upa.expressions;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class Star extends Var {

    public Star() {
        super("*");
    }

    @Override
    public Expression copy() {
        Star o = new Star();
        return o;
    }

    @Override
    public String toString() {
        return "*";
    }

}
