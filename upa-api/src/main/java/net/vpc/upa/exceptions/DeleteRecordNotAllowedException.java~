package net.vpc.upa.exceptions;


import net.vpc.upa.Entity;

public class DeleteRecordNotAllowedException extends EntityException {
    public DeleteRecordNotAllowedException(Entity entity) {
        super(entity, "delete.NotAllowed");
    }

    public DeleteRecordNotAllowedException(Entity entity, String operationName) {
        super(entity, operationName);
    }
}
