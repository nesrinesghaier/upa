package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 * @creationdate 9/16/12 5:08 PM
 */
public class TransactionException extends UPAException {
    public TransactionException() {
    }

    public TransactionException(String message, Object... parameters) {
        super(message, parameters);
    }

    public TransactionException(I18NString message, Object... parameters) {
        super(message, parameters);
    }

    public TransactionException(Throwable cause, I18NString messageId, Object... parameters) {
        super(cause, messageId, parameters);
    }
}
