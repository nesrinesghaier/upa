package net.vpc.upa.exceptions;


import net.vpc.upa.Entity;

public class UpdateRecordKeyNotFoundException extends EntityException {
    public UpdateRecordKeyNotFoundException(Entity entity, Object expressionNoFound) {
        super(entity, "update.KeyNotFound", new Object[]{expressionNoFound});
    }
}
