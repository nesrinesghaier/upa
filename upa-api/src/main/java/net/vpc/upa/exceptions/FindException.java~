package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class FindException extends UPAException {
    public FindException() {

    }

    public FindException(String message, Object... parameters) {
        super(message, parameters);
    }

    public FindException(I18NString message, Object... parameters) {
        super(message, parameters);
    }

    public FindException(Throwable cause, I18NString messageId, Object... parameters) {
        super(cause, messageId, parameters);
    }
}
