package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class PersistenceUnitException extends UPAException {
    public PersistenceUnitException() {
    }

    public PersistenceUnitException(String message, Object... parameters) {
        super(message, parameters);
    }

    public PersistenceUnitException(I18NString message, Object... parameters) {
        super(message, parameters);
    }

    public PersistenceUnitException(Throwable cause, I18NString messageId, Object... parameters) {
        super(cause, messageId, parameters);
    }
}
