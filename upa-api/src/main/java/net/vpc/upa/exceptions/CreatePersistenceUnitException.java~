package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class CreatePersistenceUnitException extends PersistenceUnitException {

    public CreatePersistenceUnitException() {
    }

    public CreatePersistenceUnitException(String message, Object... parameters) {
        super(message, parameters);
    }

    public CreatePersistenceUnitException(I18NString message, Object... parameters) {
        super(message, parameters);
    }

    public CreatePersistenceUnitException(Throwable cause, I18NString messageId, Object... parameters) {
        super(cause, messageId, parameters);
    }
}
