package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class MissingDefaultPersistenceUnitException extends PersistenceUnitException {
    public MissingDefaultPersistenceUnitException() {
        this(null);
    }

    public MissingDefaultPersistenceUnitException(Exception cause) {
        super(cause, new I18NString("MissingDefaultPersistenceUnitException"));
    }
}
