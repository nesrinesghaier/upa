package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class DropPersistenceUnitException extends PersistenceUnitException {
    public DropPersistenceUnitException() {
    }

    public DropPersistenceUnitException(String message, Object... parameters) {
        super(message, parameters);
    }

    public DropPersistenceUnitException(I18NString message, Object... parameters) {
        super(message, parameters);
    }

    public DropPersistenceUnitException(Throwable cause, I18NString messageId, Object... parameters) {
        super(cause, messageId, parameters);
    }
}
