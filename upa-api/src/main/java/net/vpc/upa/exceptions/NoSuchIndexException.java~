package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class NoSuchIndexException extends EntityException {

    public NoSuchIndexException(String entityName, String indexName, Exception cause) {
        super(cause, new I18NString("NoSuchIndexException"), entityName, indexName);
    }
}
