package net.vpc.upa.exceptions;


import net.vpc.upa.Entity;

public class InsertRecordNotAllowedException extends EntityException {
    public InsertRecordNotAllowedException(Entity entity) {
        super(entity, "insert.NotAllowed");
    }

    public InsertRecordNotAllowedException(Entity entity, String operationName) {
        super(entity, operationName);
    }
}
