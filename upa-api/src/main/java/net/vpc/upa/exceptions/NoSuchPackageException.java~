package net.vpc.upa.exceptions;

import net.vpc.upa.types.I18NString;

/**
 * @author Taha BEN SALAH <taha.bensalah@gmail.com>
 */
public class NoSuchPackageException extends EntityException {
    public NoSuchPackageException(String packageAbsoluteName, Exception cause) {
        super(cause, new I18NString("NoSuchPackageException"), packageAbsoluteName);
    }
}
