public enum ContextOperation {
    INITIALIZE,
    INSERT,
    UPDATE,
    DELETE,
    DELETE_SIMULATION,
    RENAME,
    CLONE,
    RESET,
    CLEAR,
    VALIDATE,
    CREATE_PERSISTENCE_NAME,
    FIND,
    COMMIT_STRUCTURE,
    COMMIT_STORAGE,
    CLOSE
}
